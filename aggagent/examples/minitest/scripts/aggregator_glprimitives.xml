<?xml version="1.0" encoding="UTF-8"?>
<xml>
   <glquery>
          <keyname>QUERY_SIMPLE_TASK</keyname>
          <xsl>xsl/task.simple.agg.1.xsl</xsl>
          <predicate>
               <domnode>Task</domnode>
          </predicate>
          <adapter>org.cougaar.lib.aggagent.dictionary.glquery.GenericAggServerQueryXML</adapter>
   </glquery>

   <glquery>
          <keyname>QUERY_ORG_HIERARCHY</keyname>
          <!-- NO XSL PARAMETER PROVIDED:  INDICATES THIS IS A NON-XSL GLQuery entry,
               USER-DEFINED GL QUERY PLUGIN -->
          <predicate>
               <domnode>Cluster</domnode>
                     <!-- instanceof mutually exclusive w/domnode -->
          </predicate>
          <adapter>org.cougaar.lib.aggagent.dictionary.glquery.samples.AggQueryOrgHierarchy</adapter>
   </glquery>

   <glquery>
          <keyname>QUERY_SIMPLE_ASSET</keyname>
          <xsl>xsl/asset.simple.agg.1.xsl</xsl>
          <predicate>
               <domnode>Asset</domnode>  <!-- mutually exclusive w/domnode -->
          </predicate>
          <adapter>org.cougaar.lib.aggagent.dictionary.glquery.GenericAggServerQueryXML</adapter>
   </glquery>
</xml>
