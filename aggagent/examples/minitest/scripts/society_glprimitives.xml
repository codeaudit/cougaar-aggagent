<?xml version="1.0" encoding="UTF-8"?>
<xml>
      <glquery>
          <keyname>QUERY_SIMPLE_TASK</keyname>
          <xsl>xsl/task.simple.1.xsl</xsl>
          <predicate>
               <instanceof>org.cougaar.domain.planning.ldm.plan.Task</instanceof>
          </predicate>
          <adapter>org.cougaar.lib.aggagent.dictionary.glquery.GenericQueryXML</adapter>
      </glquery>

      <glquery>
          <keyname>QUERY_SIMPLE_TASK_SAX</keyname>
          <sax>org.cougaar.lib.aggagent.bsax.samples.SampleHandler_1</sax>
          <predicate>
               <instanceof>org.cougaar.domain.planning.ldm.plan.Task</instanceof>  <!-- mutually exclusive w/domnode -->
          </predicate>
          <adapter>org.cougaar.lib.aggagent.dictionary.glquery.GenericQueryXML</adapter>
      </glquery>

      <glquery>
          <keyname>QUERY_SIMPLE_ASSET</keyname>
          <xsl>xsl/asset.simple.1.xsl</xsl>
          <predicate>
               <instanceof>org.cougaar.domain.planning.ldm.asset.Asset</instanceof>  <!-- exclusive w/domnode -->
          </predicate>
          <adapter>org.cougaar.lib.aggagent.dictionary.glquery.GenericQueryXML</adapter>
      </glquery>

      <glquery>
          <keyname>QUERY_SIMPLE_ASSET_SAX</keyname>
          <sax>org.cougaar.lib.aggagent.bsax.samples.SampleHandler_1</sax>
          <predicate>
               <instanceof>org.cougaar.domain.planning.ldm.asset.Asset</instanceof>  <!-- exclusive w/domnode -->
          </predicate>
          <adapter>org.cougaar.lib.aggagent.dictionary.glquery.GenericQueryXML</adapter>
      </glquery>

         <glquery>
          <keyname>QUERY_ORG_HIERARCHY</keyname>
          <!-- NO XSL PARAMETER PROVIDED: INDICATES THIS IS A NON-XSL GLQuery entry,
               USER-DEFINED GL QUERY PLUGIN -->
          <predicate>
               <instanceof>org.cougaar.domain.glm.ldm.asset.Organization</instanceof>
                         <!-- instanceof mutually exclusive w/domnode -->
          </predicate>
          <adapter>org.cougaar.lib.aggagent.dictionary.glquery.samples.SocietyQueryOrgHierarchy</adapter>
      </glquery>

      <glquery>
          <keyname>QUERY_SIMPLE_UNIQUE_ALL</keyname>
          <xsl>xsl/all.unique.xsl</xsl>
          <predicate>
               <instanceof>org.cougaar.core.society.UniqueObject</instanceof>  <!-- exclusive w/domnode -->
          </predicate>
          <adapter>org.cougaar.lib.aggagent.dictionary.glquery.GenericQueryXML</adapter>
      </glquery>
</xml>

